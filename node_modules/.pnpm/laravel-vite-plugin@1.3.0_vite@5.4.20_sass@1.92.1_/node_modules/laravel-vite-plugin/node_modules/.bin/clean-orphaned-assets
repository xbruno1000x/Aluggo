#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/faria/projeto-final/node_modules/.pnpm/laravel-vite-plugin@1.3.0_vite@5.4.20_sass@1.92.1_/node_modules/laravel-vite-plugin/bin/node_modules:/mnt/c/Users/faria/projeto-final/node_modules/.pnpm/laravel-vite-plugin@1.3.0_vite@5.4.20_sass@1.92.1_/node_modules/laravel-vite-plugin/node_modules:/mnt/c/Users/faria/projeto-final/node_modules/.pnpm/laravel-vite-plugin@1.3.0_vite@5.4.20_sass@1.92.1_/node_modules:/mnt/c/Users/faria/projeto-final/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/faria/projeto-final/node_modules/.pnpm/laravel-vite-plugin@1.3.0_vite@5.4.20_sass@1.92.1_/node_modules/laravel-vite-plugin/bin/node_modules:/mnt/c/Users/faria/projeto-final/node_modules/.pnpm/laravel-vite-plugin@1.3.0_vite@5.4.20_sass@1.92.1_/node_modules/laravel-vite-plugin/node_modules:/mnt/c/Users/faria/projeto-final/node_modules/.pnpm/laravel-vite-plugin@1.3.0_vite@5.4.20_sass@1.92.1_/node_modules:/mnt/c/Users/faria/projeto-final/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/clean.js" "$@"
else
  exec node  "$basedir/../../bin/clean.js" "$@"
fi
